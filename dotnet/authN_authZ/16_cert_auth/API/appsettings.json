{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "CertificateAuthentication": {
    "AllowSelfSigned": false,
    "CheckRevocation": true,
    "TrustedCertificates": [
      "certificates/root-ca.crt",
      "certificates/intermediate-ca.crt"
    ]
  }
}
